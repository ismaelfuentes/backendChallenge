{
  "name": "periscope-backend",
  "version": "1.0.0",
  "description": "Basic setup to for the backend engineer test",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon src/index.ts",
    "start": "node dist/src/index.js",
    "test": "set DEBUG=* && jest --testPathPattern=test --testPathIgnorePatterns=test/e2e --coverage",
    "test:e2e": "set DEBUG=* && jest --testPathPattern=test/e2e --silent"
  },
  "author": "Ismael Fuentes",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "mongodb": "^6.3.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "29.2.0",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  }
}
